{
    "name": "ssnepenthe/maintenance-command",
    "description": "Toggle maintenance mode from WP-CLI",
    "type": "wp-cli-package",
    "homepage": "https://github.com/ssnepenthe/maintenance-command",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": ["command.php"]
    },
    "require": {
        "php": ">=7.0",
        "wp-cli/wp-cli": "^1.1.0"
    },
    "require-dev": {
        "behat/behat": "~2.5"
    },
    "extra": {
        "commands": [
            "maintenance disable",
            "maintenance enable",
            "maintenance status",
            "maintenance toggle"
        ]
    }
}
